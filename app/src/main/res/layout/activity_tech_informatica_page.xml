<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TechInformaticaPage"
    android:background="#CC0033">


    <TextView
        android:id="@+id/textView8"
        android:layout_width="368dp"
        android:layout_height="80dp"
        android:layout_below="@+id/lijnn"
        android:text="@string/Tech_Informatica_info"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView5"
        android:layout_centerHorizontal="true"
        android:text="@string/Tech_Informatica"
        android:textColor="#FFFFFF"
        android:textSize="30sp" />


    <TextView
        android:id="@+id/textView9"
        android:layout_width="394dp"
        android:layout_height="188dp"
        android:layout_below="@+id/lijnview1"

        android:text="@string/Tech_Informatica_info2"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="289dp"
        android:layout_height="63dp"
        android:layout_below="@+id/lijnview2"
        android:layout_centerHorizontal="true"
        android:text="@string/Tech_informatica_openday"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="388dp"
        android:layout_height="78dp"
        android:layout_below="@+id/view3"
        android:text="@string/Tech_Informatica_info3"
        android:textColor="#FFFFFF" />


    <View
        android:id="@+id/lijnn"
        android:layout_width="315dp"
        android:layout_height="3dp"
        android:layout_below="@+id/textView7"
        android:background="@android:color/white"
        ></View>

    <View
        android:id="@+id/lijnview1"
        android:layout_width="315dp"
        android:layout_below="@+id/textView8"
        android:layout_height="3dp"

        android:background="@android:color/white"
        ></View>

    <View
        android:id="@+id/lijnview2"
        android:layout_width="315dp"
        android:layout_below="@+id/textView9"
        android:layout_height="3dp"

        android:layout_marginBottom="8dp"
        android:background="@android:color/white"
        ></View>

    <View
        android:id="@+id/view3"
        android:layout_width="315dp"

        android:layout_height="3dp"
        android:layout_below="@+id/textView10"
        android:background="@android:color/white"
        ></View>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="324dp"
        android:layout_height="86dp"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/appz" />

</RelativeLayout>